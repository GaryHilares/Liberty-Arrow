{
	"name": "Liberty Arrow",
	"version": "1.0",
	"description": "A customizable page blocker perfect for your needs.",
	"permissions": ["storage","tabs"],
	"background": {
		"scripts": ["static/js/background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "static/images/Liberty_Arrow_text-false_16.png",
			"32": "static/images/Liberty_Arrow_text-false_32.png",
			"48": "static/images/Liberty_Arrow_text-false_64.png",
			"128": "static/images/Liberty_Arrow_text-false_128.png"
		}
	},
	"icons": {
		"16": "static/images/Liberty_Arrow_text-false_16.png",
		"32": "static/images/Liberty_Arrow_text-false_32.png",
		"48": "static/images/Liberty_Arrow_text-false_64.png",
		"128": "static/images/Liberty_Arrow_text-false_128.png"
	},
	"manifest_version": 2
}
{
	"name": "Liberty Arrow",
	"version": "1.2",
	"description": "A customizable page blocker perfect for your needs.",
	"permissions": [
		"storage",
		"tabs"
	],
	"host_permissions": [
		"https://liberty-arrow-backend.vercel.app/"
	],
	"background": {
		"scripts": ["static/js/background.js"]
	},
	"action": {
		"default_title": "Liberty Arrow's settings",
		"default_popup": "popup.html",
		"default_icon": {
			"16": "static/images/Liberty_Arrow_text-false_16.png",
			"32": "static/images/Liberty_Arrow_text-false_32.png",
			"48": "static/images/Liberty_Arrow_text-false_48.png",
			"128": "static/images/Liberty_Arrow_text-false_128.png"
		}
	},
	"icons": {
		"16": "static/images/Liberty_Arrow_text-false_16.png",
		"32": "static/images/Liberty_Arrow_text-false_32.png",
		"48": "static/images/Liberty_Arrow_text-false_48.png",
		"128": "static/images/Liberty_Arrow_text-false_128.png"
	},
	"manifest_version": 3
}